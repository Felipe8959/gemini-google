from collections import defaultdict

def distribuir_analistas(row, limite=18):
    revisao = row['REVISÃO']
    prioridade = row['PRIORIDADE']

    if revisao == 'Sim':
        analistas_disponiveis = list(dict_status_analista.values())
        distribuicao = defaultdict(list)  # Usamos defaultdict em vez de um dicionário normal

        # Distribuir igualmente para os analistas
        for analista in analistas_disponiveis:
            distribuicao[analista] = []

        for _ in range(limite):
            # Encontrar o analista com menos atribuições
            menor_atribuicao = min(distribuicao, key=lambda x: len(distribuicao[x]))
            distribuicao[menor_atribuicao].append(prioridade)

        # Atribuir protocolos ao analista
        for analista, protocolos in distribuicao.items():
            for protocolo in protocolos:
                # atribuir protocolo ao analista aqui
                pass  # Substitua este comentário com a lógica real de atribuição

    else:
        # Lógica para outro caso aqui
        pass

    return None  # ou algo mais significativo, dependendo do fluxo do seu programa
