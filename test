from selenium import webdriver
from selenium.webdriver.common.by import By
import time

# Inicializa o navegador
driver = webdriver.Chrome()

# Abre a página inicial
driver.get('URL_DA_SUA_PAGINA')

# Guarda o identificador da janela original
janela_original = driver.current_window_handle

# Encontra e clica no botão que abre a nova janela
botao = driver.find_element(By.ID, 'ID_DO_BOTAO')
botao.click()

# Espera um momento para garantir que a nova janela foi aberta
time.sleep(2)

# Obtém todos os identificadores de janela
janelas = driver.window_handles

# Alterna para a nova janela
for janela in janelas:
    if janela != janela_original:
        driver.switch_to.window(janela)
        break

# A partir daqui, você pode realizar as interações na nova janela
# Por exemplo, confirmar a impressão, realizar verificações, etc.

# Se precisar voltar para a janela original depois de finalizar as interações:
driver.switch_to.window(janela_original)

# Continue a automação ou finalize
driver.quit()
